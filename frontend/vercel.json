{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://nauseating-degree-production.up.railway.app",
    "NEXT_PUBLIC_ENVIRONMENT": "production",
    "NEXT_PUBLIC_APP_NAME": "Oil Spill Detection System",
    "NEXT_PUBLIC_VERSION": "1.0.0"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "https://nauseating-degree-production.up.railway.app",
      "NEXT_PUBLIC_ENVIRONMENT": "production"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://nauseating-degree-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
